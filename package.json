  "dependencies": {
    "apollo-server": "^2.19.0",
    "dataloader": "^2.0.0",
    "graphql": "^15.4.0",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
